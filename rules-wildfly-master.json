{
  "repositories" : {
    "Central": "https://repo1.maven.org/maven2/",
    "JBossPublic": "https://repository.jboss.org/nexus/content/repositories/public/"
  },

  "ignoreScopes" : [ "test" ],

  "rules" : {

    "*:*" : {
      "STREAM" : "MICRO",
      "IGNORE" : [
        "[.-]SNAPSHOT",
        "[.-]CR\\d+",
        "[.-]RC\\d+",
        "[.-]b\\d+",
        "[.-]beta-?\\d+",
        "[.-]Beta-?\\d+",
        "[.-]alpha-?\\d+",
        "[.-]Alpha-?\\d+",
        "[.-]format-\\d+"
      ]
    },

    "com.h2database:*": {
      "STREAM" : "MICRO"
    },

    "com.google.guava:guava" : {
      "STREAM" : "MICRO",
      "QUALIFIER" : "jre"
    },

    "io.jaegertracing:*" : {
      "IGNORE" : "0\\.34\\.2"
    },

    "org.apache.activemq:*" : {
      "STREAM" : "MINOR",
      "IGNORE" : [
        "[.-]SNAPSHOT"
      ]
    },

    "org.bouncycastle:*" : {
      "STREAM" : "MINOR",
      "IGNORE" : "1\\.64"
    },

    "org.smalltye:jandex" : {
      "STREAM" : "MINOR"
    },

    "org.wildfly.transaction:wildfly-transaction-client" : {
      "IGNORE" : "1\\.1\\.12\\.Final-redhat-00001"
    }

  }
}
